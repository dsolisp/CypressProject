    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler"
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.js"
  ]
}